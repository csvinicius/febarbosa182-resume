{
  "name": "fb-resume",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "npm run build:prod && npm run server",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "test": "ng lint && ng test --configuration=test --code-coverage",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch --reporters dots",
    "e2e": "ng e2e",
    "ci": "npm run format:test && ng lint && ng test --configuration=test --code-coverage && npm run e2e && npm run build:prod ",
    "format:write": "prettier src/**/*.{ts,json,scss} --write",
    "format:test": "prettier src/**/*.{ts,json,scss} --list-different",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/resume/stats-es2015.json",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.6",
    "@angular/cdk": "^13.3.6",
    "@angular/common": "^13.3.6",
    "@angular/compiler": "^13.3.6",
    "@angular/core": "^13.3.6",
    "@angular/forms": "^13.3.6",
    "@angular/material": "^13.3.6",
    "@angular/platform-browser": "^13.3.6",
    "@angular/platform-browser-dynamic": "^13.3.6",
    "@angular/router": "^13.3.6",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/entity": "^13.2.0",
    "@ngrx/router-store": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "browser-detect": "^0.2.28",
    "fsevents": "^2.3.2",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.5",
    "@angular-devkit/schematics": "^13.3.5",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.6",
    "@angular/language-service": "^13.3.6",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/jasmine": "^4.0.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^17.0.32",
    "@types/uuid": "^8.3.4",
    "codelyzer": "^6.0.2",
    "eslint": "^8.15.0",
    "express": "^4.18.1",
    "husky": "^8.0.1",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "^5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-spec-reporter": "^0.0.34",
    "npm-run-all": "^4.1.5",
