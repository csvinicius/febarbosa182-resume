{
  "name": "fb-resume",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "npm run build:prod && npm run server",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "test": "ng lint && ng test --configuration=test --code-coverage",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch --reporters dots",
    "e2e": "ng e2e",
    "ci": "npm run format:test && ng lint && ng test --configuration=test --code-coverage && npm run e2e && npm run build:prod ",
    "format:write": "prettier src/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier src/**/*.{ts,json,md,scss} --list-different",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/resume/stats-es2015.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.7",
    "@angular/cdk": "^10.0.0",
    "@angular/common": "^10.0.7",
    "@angular/compiler": "^10.0.7",
    "@angular/core": "^10.0.7",
    "@angular/forms": "^10.0.7",
    "@angular/material": "^10.1.1",
    "@angular/platform-browser": "^10.0.7",
    "@angular/platform-browser-dynamic": "^10.0.7",
    "@angular/router": "^10.0.7",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^9.2.0",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@popperjs/core": "^2.4.4",
    "bootstrap": "^5.0.0-alpha1",
    "browser-detect": "^0.2.28",
    "fsevents": "^2.1.3",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "uuid": "^8.3.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular-devkit/schematics": "^10.0.5",
    "@angular/cli": "~10.0.5",
    "@angular/compiler-cli": "~10.0.7",
    "@angular/language-service": "~10.0.7",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/jasmine": "~3.5.12",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.27",
    "@types/uuid": "^8.0.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-spec-reporter": "^0.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.2",
    "ts-node": "~8.10.2",
    "tslint": "^6.1.3",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
